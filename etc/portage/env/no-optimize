# only use base optimizations
COMMON_FLAGS="-O2 -march=native -pipe"
OVERRIDE_CFLAGS="${COMMON_FLAGS}"
OVERRIDE_CXXFLAGS="${COMMON_FLAGS}"
OVERRIDE_OBJCFLAGS="${COMMON_FLAGS}"
OVERRIDE_OBJCXXFLAGS="${COMMON_FLAGS}"
OVERRIDE_FCFLAGS="${COMMON_FLAGS}"
OVERRIDE_F77FLAGS="${COMMON_FLAGS}"
OVERRIDE_LDFLAGS="-Wl,-O2,--as-needed"
OVERRIDE_RUSTFLAGS="-C target-cpu=native -C opt-level=2"
